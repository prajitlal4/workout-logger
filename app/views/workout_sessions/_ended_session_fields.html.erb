<%= form.fields_for :session_exercises do |session_exercise_form| %>
  <% session_exercise_form.object.set_details.each_with_index do |set_detail, index| %>
    <div class="set-detail">
      <%= session_exercise_form.number_field :reps, value: set_detail['reps'], name: "workout_session[session_exercises_attributes][#{session_exercise_form.index}][set_details][][reps]" %>
      <%= session_exercise_form.number_field :weight, value: set_detail['weight'], name: "workout_session[session_exercises_attributes][#{session_exercise_form.index}][set_details][][weight]" %>
      <%= session_exercise_form.text_field :note, value: set_detail['note'], name: "workout_session[session_exercises_attributes][#{session_exercise_form.index}][set_details][][note]" %>
    </div>
  <% end %>
<% end %>